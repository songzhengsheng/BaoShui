</head>
<body class="fixed-sidebar full-height-layout gray-bg" style="overflow:hidden">
    <div id="wrapper">
        <!--左侧导航开始-->
        <nav class="navbar-default navbar-static-side" role="navigation">
            <div class="nav-close">
                <i class="fa fa-times-circle"></i>
            </div>
            <div class="sidebar-collapse">
                <ul class="nav" id="side-menu">
                    <li class="nav-header">
                        <div class="dropdown profile-element">
                            <span>
                                <img alt="image" class="img-circle" src="~/Content/hplus/img/profile_small.jpg" />
                            </span>
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                                <span class="clear">
                                    <span class="block m-t-xs"><strong class="font-bold">@ViewBag.PeopleName</strong></span>
                                    <span class="text-muted text-xs block">@ViewBag.Name<b class="caret"></b></span>
                                </span>
                            </a>
                            <ul class="dropdown-menu animated fadeInRight m-t-xs">


                                <li><a class="J_menuItem" href="@Url.Action("UpdatePwd", "Sys")">修改密码</a></li>
                                <li>
                                    <a href="@Url.Action("LogOff", "Home")">退出</a>
                                </li>
                            </ul>
                        </div>
                        <div class="logo-element">
                            信息系统
                        </div>
                    </li>
                    @Html.Raw(ViewBag.menu)


                </ul>
            </div>
        </nav>
        <!--左侧导航结束-->
        <!--右侧部分开始-->
        <div id="page-wrapper" class="gray-bg dashbard-1">
            <div class="row border-bottom">
                <nav class="navbar navbar-static-top" role="navigation" style="margin-bottom: 0">
                    <div class="navbar-header">
                        <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="#"><i class="fa fa-bars"></i> </a>
                    </div>
                    @{
                        if (ViewBag.role == "7")
                        {
                            <ul class="nav navbar-top-links navbar-right">
                                <li class="dropdown">
                                    <a class="dropdown-toggle count-info" data-toggle="dropdown">
                                        <i class="fa fa-bell"></i> <span class="label label-primary">@ViewBag.sum</span>
                                    </a>
                                    <ul class="dropdown-menu dropdown-alerts">
                                        <li>
                                            <a class="J_menuItem" href="/ck/Contract/Index">
                                                <div>
                                                    <i class="fa fa-envelope fa-fw"></i> 您有 @ViewBag.Ht 条待审核合同
                                                    <span class="pull-right text-muted small"></span>
                                                </div>
                                            </a>
                                        </li>
                                        <li>
                                            <a class="J_menuItem" href="/ck/GoodsBill/GOODSBILLList">
                                                <div>
                                                    <i class="fa fa-envelope fa-fw"></i> 您有 @ViewBag.Ph 条待审核票货
                                                    <span class="pull-right text-muted small"></span>
                                                </div>
                                            </a>
                                        </li>
                                        <li>
                                            <a class="J_menuItem" href="/ck/Entrust/EntrustIndex_djlsh">
                                                <div>
                                                    <i class="fa fa-envelope fa-fw"></i> 您有 @ViewBag.Wt 条待审核委托
                                                    <span class="pull-right text-muted small"></span>
                                                </div>
                                            </a>
                                        </li>

                                    </ul>
                                </li>
                            </ul>

                        }
                        if (ViewBag.role == "3")
                        {
                            <ul class="nav navbar-top-links navbar-right">
                                <li class="dropdown">
                                    <a class="dropdown-toggle count-info" data-toggle="dropdown">
                                        <i class="fa fa-bell"></i> <span class="label label-primary">@ViewBag.Wt</span>
                                    </a>
                                    <ul class="dropdown-menu dropdown-alerts">
                                        <li>
                                            <a class="J_menuItem" href="/ck/Entrust/EntrustIndex_dsh">
                                                <div>
                                                    <i class="fa fa-envelope fa-fw"></i> 您有 @ViewBag.Wt 条待审核委托
                                                    <span class="pull-right text-muted small"></span>
                                                </div>
                                            </a>
                                        </li>

                                    </ul>
                                </li>
                            </ul>

                        }
                    }
                </nav>

            </div>
            <div class="row content-tabs">
                <button class="roll-nav roll-left J_tabLeft">
                    <i class="fa fa-backward"></i>
                </button>
                <nav class="page-tabs J_menuTabs">
                    <div class="page-tabs-content">
                        <a href="javascript:;" class="active J_menuTab" data-id="index_v1.html">首页</a>
                    </div>
                </nav>

                <button class="roll-nav roll-right J_tabRight">
                    <i class="fa fa-forward"></i>
                </button>

                <div class="btn-group roll-nav roll-right">
                    <button class="dropdown J_tabClose" data-toggle="dropdown">
                        关闭操作<span class="caret"></span>
                    </button>
                    <ul role="menu" class="dropdown-menu dropdown-menu-right">
                        <li class="J_tabshuaxin">
                            <a>刷新当前页</a>
                        </li>
                        <li class="J_tabShowActive">
                            <a>定位当前选项卡</a>
                        </li>
                        <li class="divider"></li>
                        <li class="J_tabCloseAll">
                            <a>关闭全部选项卡</a>
                        </li>
                        <li class="J_tabCloseOther">
                            <a>关闭其他选项卡</a>
                        </li>
                        <li class="">
                            <a href="@Url.Action("LogOff", "Home")">退出</a>
                        </li>
                    </ul>
                </div>

                <a href="#" class="roll-nav roll-right J_tabExit J_tabshuaxin"><i class="fa fa fa-refresh"></i> 刷新</a>
            </div>
            <div class="row J_mainContent" id="content-main">
                <iframe class="J_iframe" id="iframe0" name="iframe0" width="100%" height="100%" src="/home/HomePage" frameborder="0" data-id="index_v1.html" seamless></iframe>
            </div>

            <div class="footer">
                <div class="pull-right">
                    连云港港——@ViewBag.yuanquname&copy; 2016-2018
                </div>
            </div>
        </div>
    </div>
    <a class="J_menuItem" href="#" id="addNewTabBySon" style="visibility: hidden"></a>
    <script src="~/Content/hplus/js/jquery.min.js?v=2.1.4"></script>
    <script src="~/Content/hplus/js/bootstrap.min.js?v=3.3.6"></script>
    <script src="~/Content/hplus/js/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="~/Content/hplus/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/Content/hplus/js/plugins/layer/layer.min.js"></script>
    <script src="~/Content/hplus/js/hplus.min.js?v=4.1.0"></script>
    <script type="text/javascript" src="~/Content/hplus/js/contabs.min.js"></script>
    <script src="~/Content/hplus/js/plugins/pace/pace.min.js"></script>

    <script>
        function chakan_ht() {
            var url = "/ck/Contract/Index";
            var newTab = window.parent.document.getElementById("addNewTabBySon");
            newTab.href = url;
            newTab.innerText = "合同管理";
            newTab.click();
        }
        function chakan_ph() {
            var url = "/ck/GoodsBill/GOODSBILLList";
            var newTab = window.parent.document.getElementById("addNewTabBySon");
            newTab.href = url;
            newTab.innerText = "票货管理";
            newTab.click();
        }
        function chakan_wt() {
            var url = "/ck/Entrust/EntrustIndex_djlsh";
            var newTab = window.parent.document.getElementById("addNewTabBySon");
            newTab.href = url;
            newTab.innerText = "委托管理";
            newTab.click();
        }
    </script>

</body>

